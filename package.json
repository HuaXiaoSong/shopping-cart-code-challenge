  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "tailwind"
          ]
        }
      ],
      "no-invalid-position-at-import-rule": null
    }
