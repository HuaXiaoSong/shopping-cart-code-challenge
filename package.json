    "antd": "^5.4.6",
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "decimal.js": "^10.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
